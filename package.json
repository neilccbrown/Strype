{
    "name": "strype-editor",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --port 8081",
        "serve:microbit": "cross-env npm_config_microbit=true vue-cli-service serve --port 8081",
        "serve:python": "cross-env npm_config_python=true vue-cli-service serve --port 8081",
        "serve:python:dist:playwright": "rimraf temp-scripts/playwright-server && npm run build --python && mkdirp temp-scripts/playwright-server/editor/ && mv dist/* temp-scripts/playwright-server/editor/ && npx serve -l 8081 temp-scripts/playwright-server",
        "build": "vue-cli-service build",
        "lint:check": "vue-cli-service lint --no-fix --max-warnings 0",
        "lint:fix": "vue-cli-service lint",
        "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/localisation/**/*.json'",
        "library:make-ac-json": "cd scripts && npm install && npx tsc && (node -e \"console.log('library_arg = \\'' + process.env.LIBRARY + '\\';')\" && npx esbuild --bundle compiled/scripts/process-skulpt-api.js) | npx browser-run -s ../public > ../autocomplete.json",
        "preprocess:update-microbit-api": "python3.9 scripts/process-microbit-api.py | python3.8 scripts/add-docs-from-python.py > src/autocompletion/microbit-api.json",
        "preprocess:update-skulpt-api": "cd scripts && npm install && npx tsc && (((echo 'library_arg=null;' && npx esbuild --bundle compiled/scripts/process-skulpt-api.js) | npx browser-run  -s ../public) | python3.9 add-docs-from-python.py) > ../src/autocompletion/skulpt-api.json",
        "preprocess:build-docs": "cd docs-sphinx && make clean && make html",
        "preprocess:update-test-library-ac": "concurrently \"cross-env LIBRARY='http://localhost:8089/test-library/' npm run library:make-ac-json\" \"npm run test:cypress:serve-assets\"",
        "serve:https": "HTTPS=true vue-cli-service serve",
        "cy:run:microbit": "npx cypress run --env mode=microbit",
        "cy:run:python": "npx cypress run --env mode=python",
        "test:cypress:serve-assets": "npx serve tests/cypress/test-http-assets/ -l 8089 --cors",
        "test:cypress:serve-both": "concurrently \"npm run serve:python\" \"npm run test:cypress:serve-assets\"",
        "test:cypress:microbit-only": "cross-env WAIT_ON_TIMEOUT=90000 start-server-and-test serve:microbit http-get://127.0.0.1:8081/microbit/ cy:run:microbit",
        "test:cypress:python-only": "cross-env WAIT_ON_TIMEOUT=90000 start-server-and-test test:cypress:serve-both http-get://127.0.0.1:8081/editor/ cy:run:python",
        "test:cypress": "cross-env WAIT_ON_TIMEOUT=90000 start-server-and-test test:cypress:serve-both http-get://127.0.0.1:8081/editor/ cy:run:python && cross-env WAIT_ON_TIMEOUT=90000 start-server-and-test serve:microbit http-get://127.0.0.1:8081/microbit/ cy:run:microbit",
        "playwright:run": "npx playwright test",
        "test:playwright": "cross-env BASE_URL=http://127.0.0.1:8081/editor/ start-server-and-test serve:python:dist:playwright http-get://127.0.0.1:8081/editor/ playwright:run"
    },
    "dependencies": {
        "@azure/msal-browser": "^4.15.0",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@microbit/microbit-fs": "^0.9.2",
        "@popperjs/core": "^2.11.4",
        "@types/gapi": "^0.0.42",
        "@types/google.accounts": "^0.0.2",
        "@types/google.picker": "^0.0.39",
        "@types/jquery": "^3.5.16",
        "@types/lodash": "^4.14.191",
        "@types/object-hash": "^1.3.3",
        "@types/path-browserify": "^1.0.3",
        "@types/sortablejs": "^1.10.6",
        "@types/splitpanes": "^2.2.6",
        "@wsfe/ctree": "^2.4.1",
        "audiobuffer-to-wav": "^1.0.0",
        "axios": "^0.30.2",
        "bootstrap": "^4.6.1",
        "bootstrap-vue": "^2.16.0",
        "cheerio": "^1.0.0-rc.5",
        "core-js": "^3.28.0",
        "dapjs": "^2.2.0",
        "detect-collisions": "^9.17.0",
        "file-saver": "^2.0.2",
        "html2canvas": "^1.4.1",
        "jquery": "^3.6.3",
        "lodash": "^4.17.20",
        "lz-string": "^1.4.4",
        "object-hash": "^2.0.3",
        "path-browserify": "^1.0.1",
        "pica": "^9.0.1",
        "pinia": "^2.0.32",
        "python-shell": "^2.0.3",
        "raw-loader": "^4.0.2",
        "remove-files-webpack-plugin": "^1.5.0",
        "sortablejs": "^1.15.0",
        "splitpanes": "^2.4.1",
        "textarea-caret": "^3.1.0",
        "tigerpython-parser": "git+https://github.com/neilccbrown/TigerPython-Parser.git",
        "timed-cache": "^2.0.0",
        "ts-enum-util": "^4.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "v-blur": "^1.0.4",
        "velocity": "^0.7.3",
        "vue": "2.6.14",
        "vue-advanced-cropper": "^1.11.7",
        "vue-async-computed": "^3.9.0",
        "vue-autosize-input": "^0.1.1",
        "vue-browser-detect-plugin": "^0.1.18",
        "vue-burger-menu": "^2.0.5",
        "vue-context": "^6.0.0",
        "vue-i18n": "^8.27.1",
        "vue-input-autowidth": "^1.0.10",
        "zod": "^3.25.32"
    },
    "devDependencies": {
        "@microsoft/microsoft-graph-types": "^2.40.0",
        "@pinia/testing": "^0.0.14",
        "@playwright/test": "^1.52.0",
        "@testing-library/cypress": "^10.0.1",
        "@types/audiobuffer-to-wav": "^1.0.4",
        "@types/chai": "^4.2.11",
        "@types/file-saver": "^2.0.5",
        "@types/js-yaml": "^4.0.9",
        "@types/lz-string": "^1.3.34",
        "@types/mocha": "^5.2.4",
        "@types/node": "^22.14.0",
        "@types/pica": "^9.0.5",
        "@types/pixelmatch": "^5.2.6",
        "@types/pngjs": "^6.0.5",
        "@types/seedrandom": "^3.0.8",
        "@types/testing-library__cypress": "^5.0.13",
        "@types/textarea-caret": "^3.0.0",
        "@types/timed-cache": "^2.0.3",
        "@types/w3c-web-usb": "^1.0.6",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vue/cli-plugin-babel": "^5",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-typescript": "^5",
        "@vue/cli-plugin-unit-mocha": "^5",
        "@vue/cli-plugin-vuex": "^5",
        "@vue/cli-service": "^5.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "@vue/test-utils": "^1.3.4",
        "arkit": "^1.6.4",
        "chai": "^4.1.2",
        "chai-sorted": "^0.2.0",
        "concurrently": "^9.1.2",
        "cross-env": "^7.0.3",
        "cross-env-shell": "^7.0.3",
        "cypress": "^13.15.2",
        "cypress-fail-on-console-error": "^5.0.3",
        "cypress-split": "^1.24.18",
        "cypress-terminal-report": "^5.3.9",
        "eslint": "^7",
        "eslint-plugin-vue": "^7.20.0",
        "husky": "^7.0.4",
        "js-conditional-compile-loader": "^1.0.15",
        "lint-staged": "^11.2.6",
        "mkdirp": "^3.0.1",
        "mocha": "^10.2.0",
        "mochapack": "^2.1.4",
        "null-loader": "^4.0.1",
        "pixelmatch": "^7.1.0",
        "pngjs": "^7.0.0",
        "rimraf": "^5.0.10",
        "sass": "^1.77.0",
        "sass-loader": "^13.0.0",
        "seedrandom": "^3.0.5",
        "serve": "^14.2.4",
        "start-server-and-test": "^1.15.4",
        "typescript": "^4.9.5",
        "vue-cli-plugin-i18n": "2.1.1",
        "vue-template-compiler": "2.6.14",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,vue}": [
            "npm run lint"
        ]
    }
}
